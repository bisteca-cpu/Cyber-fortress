"VertexLitGeneric"
{
	"$basetexture"	"models/player/hvyweapon/hwm/heavy_bullets_yellow"
	"$bumpmap"	"models/player/hvyweapon/hwm/heavy_bullets_phongmask"
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .0
	"$detailblendmode" 6


	"$yellow" "0"
	"$bbqamount" "1"	
	"$two" "2.35"
	"$one" "1"
	"$absburnminusone" "1"
	"$burnminusone" "1"
	"$finalburnlevel" "1"
	"$burnlevel" "0"
	
	//phong properties
	
	"$phong"	"1"
	"$phongboost"	"1"
	"$phongexponent"	"8"
	"$phongfresnelranges"	"[.8 1.75 4]"
	
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "4"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "0.3"										// Boost for ambient cube component of rim lighting
	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$burnlevel"
		}
		"Subtract"
		{
			"srcVar1" "$burnlevel"
			"srcVar2" "$one"
			"resultVar" "$burnminusone"
		}
		"Abs"
		{
			"srcVar1" "$burnminusone" 
			"resultVar" "$absburnminusone"
		}

		"Subtract"
		{
			"srcVar1" "$one"
			"srcVar2" "$absburnminusone"
			"resultVar" "$finalburnlevel"
		}


		"Equals"
		{
			"srcVar1" "$finalburnlevel"
			"resultVar" "$detailblendfactor"
		}
			
		
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1"  "$yellow"
			"resultVar" "$color2"
		}
		

	}
}