"Water"
{
	"%keywords" "tf, bulletcrops"
//	$forcecheap 1
	
	"%tooltexture" "dev/tfwater_normal"
	"%compilewater" 1
	"$abovewater" 1
//	"$nofresnel" "1"

	"$envmap" "env_cubemap"	
	"$refracttexture" "_rt_WaterRefraction"

	"$refractamount" ".2"

	<dx90
	{
		 	"$fallbackmaterial" "maritime/nature/water_emerald_dx80"
	}


	//"$refracttint" "{39 90 91}"
	"$refractblur" "0"

//	"$reflecttexture" "_rt_WaterReflection"
//	"$reflectamount" ".1"
//	"$reflecttint" "{39 90 91}"
//	"$reflectsaturation" "[0 0 1]"

	"$scale" "[1 1]"

//	"$bumpmap" "water/tfwater001_dudv"
	"$normalmap" "water/tfwater001_normal"

	"$surfaceprop" "water"
	"$bottommaterial" "maritime/nature/water_emerald_beneath.vmt"
	"$bumpframe" "0"

	"$fogenable" 1
	"$fogcolor" "{15 72 74}"
	"$fogstart" "1"
	"$fogend" "300"

	 "$temp" "[0 0]"
	 "$curr" 0.0
	 "$curr2" 0.0



	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$normalmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}

		"Sine"
		{
			"sineperiod" "24"
			"sinemin" -0.5
			"sinemax" 0.5
			"resultVar" "$curr"
		}
		"Sine"
		{
			"sineperiod" "16"
			"sinemin" 0.5
			"sinemax" -0.5
			"resultVar" "$curr2"
		}

		"Equals"
		{
			"srcVar1" "$curr2"
			"resultVar" "$temp[0]"
		}
		"Equals"
		{
			"srcVar1" "$curr"
			"resultVar" "$temp[1]"
		}

 
		"TextureTransform"
		{
			"translateVar" "$temp"
			"resultVar" "$bumptransform"
		}  

//		"TextureScroll"
//		{
//			"texturescrollvar" "$bumptransform"
//			"texturescrollrate" .1
//			"texturescrollangle" 45.00
//		}
		"WaterLOD"
		{
			// fixme!  This has to be here, or material loading barfs.
			"dummy" 0
		}
	}
}

