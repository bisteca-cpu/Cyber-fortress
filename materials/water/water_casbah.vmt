	"$scroll1" "[.01 .01 .05]"
	"$scroll2" "[-.025 .025 .075]"
"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$normalmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}

		"Sine"
		{
			"sineperiod" "24"
			"sinemin" -0.5
			"sinemax" 0.5
			"resultVar" "$curr"
		}
		"Sine"
		{
			"sineperiod" "16"
			"sinemin" 0.5
			"sinemax" -0.5
			"resultVar" "$curr2"
		}

		"Equals"
		{
			"srcVar1" "$curr2"
			"resultVar" "$temp[0]"
		}
		"Equals"
		{
			"srcVar1" "$curr"
			"resultVar" "$temp[1]"
		}

 
		"TextureTransform"
		{
			"translateVar" "$temp"
			"resultVar" "$bumptransform"
		}  

